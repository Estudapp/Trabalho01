ALTER TABLE Matricula ADD CONSTRAINT FK_Matricula_0
    FOREIGN KEY (FK_Disciplina_Id_disciplina)
    REFERENCES Disciplina (Id_disciplina)
    ON DELETE SET NULL ON UPDATE CASCADE;
 
